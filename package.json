{
  "name": "spellbook-vuetify",
  "version": "0.0.0",
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "npm run build-storybook && node node_modules/my-spellbook/src/spells-core/build-postprocessing.js",
    "generating-stories": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1  node generating-stories.mjs",
    "deploy": "push-dir --dir=storybook-static --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "@mdi/font": "7.0.96",
    "my-spellbook": "^0.0.22",
    "roboto-fontface": "*",
    "vue": "^3.3.0",
    "vuetify": "^3.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.4.0",
    "@storybook/addon-essentials": "^8.1.2",
    "@storybook/addon-interactions": "^8.1.2",
    "@storybook/addon-links": "^8.1.2",
    "@storybook/blocks": "^8.1.2",
    "@storybook/test": "^8.1.2",
    "@storybook/vue3": "^8.1.2",
    "@storybook/vue3-vite": "^8.1.2",
    "@vitejs/plugin-vue": "^4.5.0",
    "cross-env": "^7.0.3",
    "pre-commit": "^1.2.2",
    "push-dir": "^0.4.1",
    "sass": "^1.69.0",
    "storybook": "^8.1.2",
    "unplugin-fonts": "^1.1.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.0",
    "vite-plugin-vuetify": "^2.0.0"
  }
}
